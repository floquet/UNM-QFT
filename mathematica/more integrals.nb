Cell[CellGroupData[{

Cell["excursion for finite d\[Phi]", "Subsection",
 CellChangeTimes->{{3.692842222120076*^9, 3.692842230462962*^9}}],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Block", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"as", "=", "1"}], ",", 
     RowBox[{"at", "=", "1"}], ",", 
     RowBox[{"M", "=", "1"}], ",", 
     RowBox[{"m", "=", "1"}], ",", 
     RowBox[{"\[Phi]", "=", "0"}], ",", 
     RowBox[{"g\[Phi]", "=", "0"}]}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"H", "=", 
     RowBox[{
      FractionBox[
       RowBox[{
        SuperscriptBox["p", "2"], "+", 
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox["g\[Phi]", "as"], ")"}], "2"]}], "2"], "+", 
      RowBox[{
       SuperscriptBox["m", "4"], 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"1", "-", 
            SuperscriptBox[
             RowBox[{"(", 
              FractionBox["\[Phi]", "M"], ")"}], "2"]}], ")"}], 
          RowBox[{
           RowBox[{"-", "1"}], "/", "2"}]], "-", "1"}], ")"}]}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{"\"\<H = \>\"", ",", "H"}], "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
     RowBox[{"\"\<integrand = \>\"", ",", 
      RowBox[{"H", " ", 
       RowBox[{"Cos", "[", 
        RowBox[{
         SuperscriptBox["as", "3"], "d\[Phi]", " ", "p"}], "]"}], 
       RowBox[{"Exp", "[", 
        RowBox[{
         RowBox[{"-", "at"}], " ", "H", " ", 
         SuperscriptBox["as", "3"]}], "]"}]}]}], "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"Print", "[", 
